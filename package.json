{
  "name": "video-downloader-pro",
  "version": "2.0.0",
  "description": "Baixador de vídeos universal para YouTube, TikTok, Instagram, Facebook, Vimeo e mais. Com suporte a cookies do navegador e interface web.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test-download.js",
    "check-deps": "node check-dependencies.js",
    "cleanup": "node cleanup-downloads.js",
    "update-ytdlp": "python3 -m pip install -U --pre \"yt-dlp[curl-cffi]\"",
    "check-browsers": "node -e \"console.log('Navegadores disponíveis:', require('./check-dependencies.js').getAvailableBrowsers ? require('./check-dependencies.js').getAvailableBrowsers() : 'Script não implementado')\""
  },
  "keywords": [
    "video",
    "downloader",
    "youtube",
    "tiktok",
    "instagram",
    "facebook",
    "vimeo",
    "yt-dlp",
    "web"
  ],
  "author": "Fábio Brito <fjbrit@example.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "optionalDependencies": {
    "chalk": "^4.1.2",
    "dotenv": "^16.3.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fjbrit/meu-downloader.git"
  },
  "bugs": {
    "url": "https://github.com/fjbrit/meu-downloader/issues"
  },
  "homepage": "https://github.com/fjbrit/meu-downloader#readme"
}